@model IEnumerable<EFExampleProject.Models.DoctorPatient>

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <p>
            @foreach (var item in Model)
            {
                @item.Doctor_Id
                <br />
                foreach (var p in item.Patients)
                {
                    @p.name
                    <br />
                    @p.phone
                }
                <hr />
            }
        </p>
    </div>
    

</body>
</html>
